{
	"name": "Familiar",
	"type": "dark",
	"colors": {
		"editor.background": "#282A2E",
		"editor.foreground": "#D4D4D4",
		"editor.inactiveSelectionBackground": "#3d4355",
		"editorIndentGuide.background": "#373b41",
		"editorIndentGuide.activeBackground": "#707070",
		"editor.selectionHighlightBackground": "#e0cffd13",

		"input.background": "#19191d",
		"input.foreground": "#93A1A1",
		"tab.activeForeground": "#E1E6E3",
		"tab.activeBackground": "#282A2E",
		"tab.inactiveForeground": "#7C7C88",
		"tab.inactiveBackground": "#19191d",
		"tab.border": "#19191d",
		"activityBar.background": "#19191d",
		"statusBarItem.prominentBackground": "#19191d",
		"statusBarItem.prominentHoverBackground": "#19191d",

		"editorWidget.background": "#282A2E",
		"editorCursor.foreground": "#fccbdf",
		"editorWhitespace.foreground": "#7C7C88",
		"editor.lineHighlightBackground": "#32353a9c",
		"editorLineNumber.activeForeground": "#949494",
		"editor.selectionBackground": "#454c61",
		"editor.foldBackground": "#454c6100",
		"minimap.selectionHighlight": "#454c61",
		"editorHoverWidget.background": "#282A2E",
		"editorMarkerNavigationError.background": "#AB395B",
		"editorMarkerNavigationWarning.background": "#7d7e5b",
		"editor.wordHighlightBackground": "#6f005caa",
		"editor.wordHighlightStrongBackground": "#6f005caa",

		"list.dropBackground": "#383B3D",
		"activityBarBadge.background": "#007ACC",
		"sideBarTitle.foreground": "#BBBBBB",
		"input.placeholderForeground": "#A6A6A6",
		"menu.background": "#282a2e",
		"menu.foreground": "#CCCCCC",
		"statusBarItem.remoteForeground": "#FFF",
		"statusBarItem.remoteBackground": "#16825D",
		"sideBarSectionHeader.background": "#0000",
		"sideBarSectionHeader.border": "#ccc3",
		"tab.lastPinnedBorder": "#ccc3"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#E1E6E3"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.other.normal",
				"variable.other",
				"variable",
				"punctuation.separator.variable"
			],
			"settings": {
				"foreground": "#E1E6E3"
			}
		},
		{
			"name": "Field",
			"scope": [
				"meta.definition.variable.name",
				"support.variable",
				"entity.name.variable",
				"entity.other.attribute-name",
				"entity.name.tag",
				"variable.other.property"
			],
			"settings": {
				"foreground": "#E1E6E3"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.language.self keyword.other",
				"variable.language.self",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#e5c9e1"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
			],
			"settings": {
				"foreground": "#E6D895"
			}
		},
		{
			"name": "Control Flow",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#C495E6"
			}
		},
		{
			"name": "Other Keywords",
			"scope": [
				"storage.type",
				"keyword.operator.noexcept",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.other.using",
				"keyword.other.operator",
				"entity.name.operator",
				"keyword",
			],
			"settings": {
				"foreground": "#C495E6"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"entity.name.function.member",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal",
				"support.function.any-method",
				"keyword.other.special-method",
				"meta.require",
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java",
			],
			"settings": {
				"foreground": "#e5c18e"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.class",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#98B0F2"
			}
		},
		{
			"name": "Meta",
			"scope": [
				"meta.attribute",
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#7C7C88"
			}
		},
		{
			"name": "Macros",
			"scope": [
				"entity.name.function.macro",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#E6D895"
			}
		},
		{
			"name": "Numerical constants",
			"scope": [
				"constant.numeric",
				"constant.character.escape",
				"meta.preprocessor.numeric"
			],
			"settings": {
				"foreground": "#85CCC2"
			}
		},
		{
			"name": "String literals",
			"scope": [
				"string",
				"string.tag",
				"string.value",
				"meta.preprocessor.string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#B9D977"
			}

		},
		{
			"name": "Language constants",
			"scope": [
				"constant.language",
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "User defined constants and enums",
			"scope": [
				"variable.other.constant",
				"variable.other.enummember",
				"constant.character",
				"constant.other",
				"constant.language",
				"variable.other.global",
			],
			"settings": {
				"foreground": "#AEEACB"
			}
		},
		{
			"name": "Regular expression groups",
			"scope": [
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp",
				"keyword.operator.or.regexp",
				"keyword.control.anchor.regexp",
				"keyword.operator.quantifier.regexp",
				"constant.character.character-class.regexp",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.set.regexp"
			],
			"settings": {
				"foreground": "#CE9178"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#CE9178"
			}
		},

		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#CE9178"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#7C7C88"
			}
		},

		{
			"name": "Bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f06431"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#98676a"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Code",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#889b4a"
			}
		},
		{
			"name": "Link Text",
			"scope": "string.other.link",
			"settings": {
				"foreground": "#dc3958"
			}
		},
		{
			"name": "Link Url",
			"scope": "meta.link",
			"settings": {
				"foreground": "#f79a32"
			}
		},
		{
			"name": "Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#dc3958"
			}
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#f79a32"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#889b4a"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#dc3958"
			}
		},
		{
			"name": "Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#98676a"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#dc3958"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#E6D895",
		"stringLiteral": "#B9D977",
		"customLiteral": "#AEEACB",
		"numberLiteral": "#85CCC2",
		"selfKeyword": "#e5c9e1",
	}
}
